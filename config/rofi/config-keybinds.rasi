/* Main Config - For Keybinds generation */

@import "~/.config/rofi/config.rasi"  


/* ---- Mainbox ---- */
mainbox {
  children:
    [ "inputbar", "message", "listview"];
}

/* ---- Entry ---- */
entry {
  expand: true;
  placeholder: " ðŸ§® Search Keybinds";
}

/* ---- Listview ---- */
listview {
  columns: 2;
  lines: 12;
  fixed-height: false;
  scrollbar: true;
}

window {
    width: 90%;
}

/* ---- Elements ---- */
element {
  orientation: horizontal;
}

element-icon {
  	size: 0%;
}

element-text {
    horizontal-align: 0.0;
    vertical-align: 0.5;
}